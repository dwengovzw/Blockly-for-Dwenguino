{
    "compilerOptions": {
      "pretty": true,
      "outDir": "./Blockly-for-Dwenguino/DwenguinoIDE/js/dist",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es6",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "importHelpers": true,
      "declaration": true,
      "declarationMap": true,
      "noEmitOnError": true,
      "strict": true,
      "allowJs": true,
      "moduleResolution": "node",
      "types": ["jquery"],
      "lib": [
        "DOM",
        "ES6"
      ]
    },
    "exclude": [
      "**/node_modules/*",
      "./Blockly-for-Dwenguino/DwenguinoIDE/js/dist",
      "./Blockly-for-Dwenguino/dashboards",
      "**/.git/*",
      "**/dist/*",
      "./Blockly-for-Dwenguino/blockly/*",
      "./Blockly-for-Dwenguino/closure/*"
    ]
  }