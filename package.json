{
  "name": "dwenguinoblockly-package",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.13.4"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/tests/unit_tests/**/*.test.js",
      "<rootDir>/tests/integration_tests/**/*.test.js",
      "<rootDir>/tests/end_to_end_tests/**/*.test.js"
    ],
    "transform": {},
    "verbose": true,
    "globalSetup": "./tests/util/puppeteer_setup.js",
    "globalTeardown": "./tests/util/puppeteer_teardown.js",
    "testEnvironment": "./tests/util/puppeteer_environment.js",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-puppeteer"
  },
  "description": "Logging server which provides unique session ids and receives logging data for all sessions running in the online version of google blockly",
  "main": "./backend/index.js",
  "scripts": {
    "test": "node --input-type module --experimental-vm-modules -r dotenv/config node_modules/jest/bin/jest.js --setupFiles dotenv/config --detectOpenHandles",
    "start": "node --experimental-vm-modules -r dotenv/config backend/index.js dotenv_config_path=backend/.env",
    "build": "webpack --mode production --progress --config webpack.config.cjs",
    "build-dev": "webpack --mode development --progress --stats-error-details --config webpack.config.dev.cjs"
  },
  "keywords": [
    "logging",
    "json",
    "dwengo"
  ],
  "author": "Tom Neutens",
  "license": "ISC",
  "bin": {
    "db-start": "./cli.js"
  },
  "dependencies": {
    "@microsoft/fast-components": "^2.30.6",
    "@microsoft/fast-element": "^1.11.0",
    "axios": "^1.1.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "browser-sync-close-hook": "^1.0.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.1",
    "csv-parser": "^2.3.2",
    "csvtojson": "^2.0.10",
    "debugjs": "github:amasad/debugjs",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "fast-xml-parser": "^3.20.3",
    "helmet": "^3.22.0",
    "i18n-x": "^0.1.6",
    "interactjs": "^1.6.2",
    "jquery": "^3.6.0",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^0.5.5",
    "monaco-editor": "^0.31.1",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "mongoose": "^6.5.2",
    "node-pre-gyp": "^0.17.0",
    "nodemailer": "^6.4.18",
    "octokit": "^2.0.10",
    "open": "^7.0.0",
    "opener": "^1.5.1",
    "pm2": "^5.1.2",
    "popper.js": "^1.14.3",
    "regenerator": "^0.14.4",
    "style-loader": "^3.3.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-classes": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.14.6",
    "@puppeteer/replay": "^1.3.0",
    "@shelf/jest-mongodb": "^2.0.1",
    "@types/jquery": "^3.5.14",
    "@types/w3c-web-serial": "^1.0.3",
    "@types/webpack-env": "^1.18.0",
    "auto-bind": "^5.0.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.0.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "bent": "^7.3.12",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.8",
    "jest": "^27.0.5",
    "jest-puppeteer": "^6.1.1",
    "jsdoc": "^3.5.5",
    "mini-css-extract-plugin": "^2.6.1",
    "mongodb-memory-server": "^7.3.0",
    "open-cli": "^7.0.1",
    "puppeteer": "^16.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^1.0.0",
    "supertest": "^6.1.3",
    "ts-loader": "^9.4.1",
    "tslib": "^2.4.1",
    "typescript": "^4.6.3",
    "v8-profiler-node8": "^7.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwengovzw/Blockly-for-Dwenguino.git"
  }
}
