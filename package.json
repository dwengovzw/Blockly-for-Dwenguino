{
  "name": "dwenguinoblockly-package",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.13.4"
  },
  "description": "Logging server which provides unique session ids and receives logging data for all sessions running in the online version of google blockly",
  "main": "./backend/index.js",
  "scripts": {
    "test": "npm run test-frontend && npm run test-backend",
    "test-frontend": "npm run test-frontend-unit && npm run test-puppeteer",
    "test-frontend-unit": "node --input-type module --experimental-vm-modules -r dotenv/config node_modules/jest/bin/jest.js --config ./tests/unit/Blockly-for-Dwenguino/jest.config.json --setupFiles dotenv/config --forceExit --detectOpenHandles",
    "test-puppeteer": "node --input-type module --experimental-vm-modules -r dotenv/config node_modules/jest/bin/jest.js --config ./tests/end_to_end/puppeteer/jest-puppeteer.config.json --setupFiles dotenv/config --forceExit --detectOpenHandles",
    "test-backend": "npm run test-backend-unit",
    "test-backend-unit": "ENV_FILE=environments/test.env node --input-type module --experimental-vm-modules -r dotenv/config node_modules/jest/bin/jest.js --config ./tests/unit/backend/jest.config.json --setupFiles dotenv/config --forceExit --detectOpenHandles",
    "build-dev": "npm run build-dev-frontend && npm run build-dev-backend",
    "build-dev-frontend": "webpack --mode development --progress --stats-error-details --config webpack.config.dev.cjs",
    "build-dev-backend": "tsc --sourcemap --project backend/tsconfig.json",
    "build": "npm run build-frontend && npm run build-backend",
    "build-frontend": "webpack --mode production --progress --config webpack.config.cjs",
    "build-backend": "tsc --project backend/tsconfig.json",
    "devstart": "npm run build-dev-backend && npm run start",
    "start": "node --experimental-vm-modules -r dotenv/config backend/dist/index.js"
  },
  "keywords": [
    "logging",
    "json",
    "dwengo"
  ],
  "author": "Tom Neutens",
  "license": "MIT",
  "bin": {
    "db-start": "./cli.js"
  },
  "dependencies": {
    "@microsoft/fast-components": "^2.30.6",
    "@microsoft/fast-element": "^1.11.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
