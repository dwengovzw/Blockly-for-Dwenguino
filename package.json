{
  "name": "dwenguinoblockly-package",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "16.4.0",
    "npm": "6.13.4"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/tests/unit_tests/**/*.test.js",
      "<rootDir>/tests/integration_tests/**/*.test.js",
      "<rootDir>/tests/end_to_end_tests/**/*.test.js"
    ],
    "preset": "@shelf/jest-mongodb",
    "transform": {},
    "verbose": true
  },
  "description": "Logging server which provides unique session ids and receives logging data for all sessions running in the online version of google blockly",
  "main": "./backend/index.js",
  "scripts": {
    "test": "node --input-type module --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "start": "node --experimental-vm-modules backend/index.js",
    "build": "webpack --debug --progress --config webpack.config.cjs",
    "build-dev": "webpack --debug --progress --config webpack.config.dev.cjs"
  },
  "keywords": [
    "logging",
    "json",
    "dwengo"
  ],
  "author": "Tom Neutens",
  "license": "ISC",
  "bin": {
    "db-start": "./cli.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "babel-jest": "^27.0.5",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "browser-sync-close-hook": "^1.0.5",
    "chrome-launcher": "^0.13.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.1",
    "csv-parser": "^2.3.2",
    "csvtojson": "^2.0.10",
    "debugjs": "github:amasad/debugjs",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "i18n-x": "^0.1.6",
    "interactjs": "^1.6.2",
    "jquery": "^3.6.0",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^0.5.5",
    "mongoose": "^5.7.12",
    "node-pre-gyp": "^0.17.0",
    "nodemailer": "^6.4.18",
    "open": "^7.0.0",
    "opener": "^1.5.1",
    "popper.js": "^1.14.3",
    "pug": "^3.0.2",
    "regenerator": "^0.14.4",
    "uuid": "^3.3.3",
    "v8-profiler-node8": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-classes": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@shelf/jest-mongodb": "^2.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "bent": "^7.3.12",
    "electron": "^9.0.2",
    "jest": "^27.0.5",
    "jsdoc": "~3.6.6",
    "mongodb-memory-server": "^7.3.0",
    "open-cli": "^6.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "supertest": "^6.1.3",
    "webpack": "^5.39.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^1.16.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwengovzw/Blockly-for-Dwenguino.git"
  }
}
